{
  "name": "@tikked/persistency",
  "version": "0.0.1",
  "description": "A feature flag thingy",
  "main": "dist/src/index.js",
  "dependencies": {
    "@tikked/core": "0.0.1",
    "axios": "1.8.4",
    "inversify": "6.2.2",
    "inversify-express-utils": "6.5.0",
    "jsonschema": "1.4.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.2"
  },
  "private": true,
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": "src",
    "reporter": [
      "lcov",
      "text"
    ],
    "temp-dir": "./.nyc_output/",
    "all": true
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist .nyc_output coverage",
    "count": "cloc --by-file-by-lang src",
    "coverage": "nyc mocha",
    "lint-fix": "eslint --fix ./src/**",
    "lint": "eslint ./src/**",
    "test": "mocha",
    "prepare": "yarn build",
    "pretty": "prettier-eslint ./{src,test}/**/*.{ts,js,json} ./.*.{ts,js,json} --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tikked/tikked.git"
  },
  "keywords": [
    "feature-flags",
    "feature-toggle"
  ],
  "author": "Alex Bondo Andersen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tikked/tikked/issues"
  },
  "homepage": "https://github.com/tikked/tikked#readme",
  "files": [
    "dist/*"
  ],
  "types": "dist/src/index.d.ts"
}
